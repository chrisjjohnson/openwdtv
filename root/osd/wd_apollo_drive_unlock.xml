<?xml version="1.0"?>
<page background="image/villa_bg.jpg" type="basic">
<ctrlgrp>
	<button valkey="OK"
	focused_bg="image/Villa_button_blue_f_02.png"
	normal_bg="image/Villa_button_blue_n_02.png"/>
	<button valkey="CANCEL"
	focused_bg="image/Villa_button_blue_f_02.png"
	normal_bg="image/Villa_button_blue_n_02.png"/>
</ctrlgrp>
<image image="image/header.png" x="0" y="0" w="1280" h="100" bg="1"/>
<image image="image/footer.png" x="0" y="620" w="1280" h="100" bg="1"/>
<text text="$$Unlock your WD drive" x="100" y="60" w="550" h="30" align="left" fontsize="26" textcolor="0xcccccc"/>
<text text="$$Would you like to unlock this WD drive?" x="300" y="200" w="680" h="60" fontsize="26" textcolor="0xcccccc" align="hcenter" numlines="2"/>
