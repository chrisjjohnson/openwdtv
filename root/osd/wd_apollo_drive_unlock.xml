<imgbtn x="310" y="400" w="310" h="50" image="@@bg-OK" text="$$OK" align="hcenter" fontsize="26" textcolor="0xcccccc" active="@@act-OK" actnoseltextcolor="0xcccccc"/>
<imgbtn x="660" y="400" w="310" h="50" image="@@bg-CANCEL" text="$$Cancel" align="hcenter" fontsize="26" textcolor="0xcccccc" active="@@act-CANCEL" actnoseltextcolor="0xcccccc"/>
</page>
