<?xml version="1.0"?>
<page background="image/villa_bg.jpg">
<ctrlgrp lbmask="l">
	<button valkey="FIRSTNAME"
	focused_bg="image/browser_list_btn.png"/>
	<linebreak/>
	<button valkey="LASTNAME"
	focused_bg="image/browser_list_btn.png"/>
	<linebreak/>
	<button valkey="EMAIL"
	focused_bg="image/browser_list_btn.png"/>
	<linebreak/>
	<button valkey="OK"
	focused_bg="image/Villa_button_blue_f_02.png"
	normal_bg="image/Villa_button_blue_n_02.png"/>
	<button valkey="CANCEL"
	focused_bg="image/Villa_button_blue_f_02.png"
	normal_bg="image/Villa_button_blue_n_02.png"/>
</ctrlgrp>
<image image="image/header.png" x="0" y="0" w="1280" h="145" bg="1"/>
<text text="$$Settings" x="100" y="60" w="550" h="30" fontsize="26" textcolor="0xcccccc" align="left"/>
<image image="image/settings.png" x="100" y="100" w="24" h="40" bg="1"/>
<text text="$$Register Product" x="144" y="100" w="800" h="40" align="left" fontsize="22" textcolor="0x4d4d4d"/>
<wrapper x="100" w="1080" h="40">
	<image image="image/browser_list_line.png" y="174"/>
	<image image="image/browser_list_line.png" y="214"/>
	<image image="image/browser_list_line.png" y="254"/>
	<image image="image/browser_list_line.png" y="294"/>
	<image image="image/browser_list_line.png" y="334"/>
</wrapper>
<table x="100" y="174" w="1080" h="120" cols="1" rows="3" colspacing="0" rowspacing="0">
	<image image="@@bg-FIRSTNAME"/>
	<image image="@@bg-LASTNAME"/>
	<image image="@@bg-EMAIL"/>
</table>
<table x="110" y="174" w="500" h="200" cols="1" rows="5" colspacing="0" rowspacing="0" align="left" xmargin="0" fontsize="26" textcolor="0xcccccc">
	<text text="$$First Name"/>
	<text text="$$Last Name"/>
	<text text="$$Email"/>
	<text text="$$Serial Number"/>
	<text text="$$Mac Address"/>
</table>
<imgbtn image="@@bg-OK" text="$$Submit" textcolor="0xcccccc" x="310" y="450" w="310" h="50" fontsize="22" align="hcenter"/>
<imgbtn image="@@bg-CANCEL" text="$$Cancel" textcolor="0xcccccc" x="660" y="450" w="310" h="50" fontsize="22" align="hcenter"/>
<wrapper x="670" w="500" h="40" align="right" delay="2">
	<text text="@@first_name_value" y="174" fontsize="26" textcolor="0xcccccc" speed="@@sel-FIRSTNAME"/>
	<text text="@@last_name_value" y="214" fontsize="26" textcolor="0xcccccc" speed="@@sel-LASTNAME"/>
	<text text="@@email_value" y="254" fontsize="26" textcolor="0xcccccc" speed="@@sel-EMAIL"/>
	<text text="@@serial_number_value" y="294" fontsize="26" textcolor="0xcccccc" speed="@@sel-OK"/>
	<text text="@@mac_address_value" y="334" fontsize="26" textcolor="0xcccccc" speed="@@sel-CANCEL"/>
</wrapper>
<include filename="inc_logo.xml"/>
</page>
