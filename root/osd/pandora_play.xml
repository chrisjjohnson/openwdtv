<?xml version="1.0"?>
<page background="image/villa_bg.jpg">
<ctrlgrp valkey="options" autoenter="1" lbmask="LR"> 
	<button valkey="CMD0"
	lightbar_bg="image/pandora_ontrols_select_button.png"
	focused_bg="image/pandora_ontrols_thumb_up_icon.png"
	disabled_bg="image/pandora_ontrols_thumb_up_icon.png"
	normal_bg="image/pandora_ontrols_thumb_up_icon.png"
	group_valkey="buttons"
	group_focused_text="I like this song"/>
	<button valkey="CMD1"
	lightbar_bg="image/pandora_ontrols_select_button.png"
	focused_bg="image/pandora_ontrols_thumb_down_icon.png"
	disabled_bg="image/pandora_ontrols_thumb_down_icon.png"
	normal_bg="image/pandora_ontrols_thumb_down_icon.png"
	group_valkey="buttons"
	group_focused_text="I don't like this song"/>
	<button valkey="CMD2"
	lightbar_bg="image/pandora_ontrols_select_button.png"
	focused_bg="image/pandora_ontrols_bookmark_icon.png"
	disabled_bg="image/pandora_ontrols_bookmark_icon.png"
	normal_bg="image/pandora_ontrols_bookmark_icon.png"
	group_valkey="buttons"
	group_focused_text="Bookmark"/>
	<button valkey="CMD3"
	lightbar_bg="image/pandora_ontrols_select_button.png"
	focused_bg="image/pandora_ontrols_Don't_Play_for_a_month_icon.png"
	disabled_bg="image/pandora_ontrols_Don't_Play_for_a_month_icon.png"
	normal_bg="image/pandora_ontrols_Don't_Play_for_a_month_icon.png"
	group_valkey="buttons"
	group_focused_text="I'm tired of this song"/>
	<button valkey="CMD4"
	lightbar_bg="image/pandora_ontrols_select_button.png"
	focused_bg="image/pandora_ontrols_info_icon.png"
	disabled_bg="image/pandora_ontrols_info_icon.png"
	normal_bg="image/pandora_ontrols_info_icon.png"
	group_valkey="buttons"
	group_focused_text="Why was this song selected?"/>
	<linebreak/>
	<button valkey="QML0"
	lightbar_bg="image/villa_options_menu_select_bar.png"
	focused_bg="image/villa_options_menu_pencil_icon.png"
	normal_bg="image/villa_options_menu_pencil_icon.png"
	group_valkey="options"
	group_focused_text="Rename station"/>
	<button valkey="QML1"
	lightbar_bg="image/villa_options_menu_select_bar.png"
	focused_bg="image/villa_options_menu_delete_station_icon.png"
	normal_bg="image/villa_options_menu_delete_station_icon.png"
	group_valkey="options"
	group_focused_text="Delete station"/>
	<linebreak/>
</ctrlgrp>
<image image="image/header.png" x="0" y="0" w="1280" h="100" bg="1"/>
