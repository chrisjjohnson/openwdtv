<?xml version="1.0"?>
<page background="image/villa_bg.jpg" wait_seconds="300">
<ctrlgrp>
	<button valkey="YES_TO_ONE" focused_bg="image/Villa_button_blue_f_02.png"
	normal_bg="image/Villa_button_blue_n_02.png"/>
	<button valkey="YES_TO_ALL" focused_bg="image/Villa_button_blue_f_02.png"
	normal_bg="image/Villa_button_blue_n_02.png"/>
	<linebreak/>
	<button valkey="SKIP_ONE" focused_bg="image/Villa_button_blue_f_02.png"
	normal_bg="image/Villa_button_blue_n_02.png"/>
	<button valkey="CANCEL" focused_bg="image/Villa_button_blue_f_02.png"
	normal_bg="image/Villa_button_blue_n_02.png"/>
</ctrlgrp>
<image image="image/header.png" x="0" y="0" w="1280" h="100" bg="1"/>
<image image="image/footer.png" x="0" y="620" w="1280" h="100" bg="1"/>
<text text="$$File Management" x="100" y="60" w="350" h="30" fontsize="26" textcolor="0xcccccc"/>
<table x="200" y="200" w="880" h="90" textcolor="0xcccccc" fontsize="26" rows="3" cols="1">
	<text text="$$This folder already contains a file named:" align="hcenter" numlines="1"/>
	<text text="@@text-dest" align="hcenter" numlines="1"/>
	<text text="$$Would you like to overwrite it?" align="hcenter" numlines="1"/>
</table>
<table x="310" y="400" w="660" h="120" rows="2" cols="2" align="hcenter" fontsize="26" actnoseltextcolor="0xcccccc">
	<imgbtn image="@@bg-YES_TO_ONE" text="$$YES" active="@@act-YES_TO_ONE"/>
	<imgbtn image="@@bg-YES_TO_ALL" text="$$Yes to all" active="@@act-YES_TO_ALL"/>
	<imgbtn image="@@bg-SKIP_ONE" text="$$NO" active="@@act-SKIP_ONE"/>
	<imgbtn image="@@bg-CANCEL" text="$$Cancel" active="@@act-CANCEL"/>
</table>
<include filename="inc_messagebox.xml"/>
</page>
