<?xml version="1.0"?>
<page background="image/villa_bg.jpg" type="basic">
<ctrlgrp lbmask="l" more_select="right">
	<button valkey="new_account"
	focused_bg="image/browser_list_btn.png"/>
	<linebreak/>
	<button valkey="old_account"
	focused_bg="image/browser_list_btn.png"/>
</ctrlgrp>
<image image="image/header.png" x="0" y="0" w="1280" h="100" bg="1"/>
<image image="image/footer.png" x="0" y="620" w="1280" h="100" bg="1"/>
<!--<image image="image/pandora_logo.png" x="69" y="20" w="63" h="60"/>-->
<text text="@@title" x="100" y="60" w="550" h="30" fontsize="26" textcolor="0xcccccc"/>
<image x="99" y="200" w="1082" h="42" image="image/browser_list_line.png"/>
<imgbtn image="@@bg-new_account" text="@@new_account_text" textcolor="0xcccccc" x="99" y="200" w="1082" h="42" fontsize="26" selected_fontsize="26" align="hcenter" active="@@act-new_account"/>
<image x="99" y="245" w="1082" h="42" image="image/browser_list_line.png"/>
<imgbtn image="@@bg-old_account" text="@@old_account_text" textcolor="0xcccccc" x="99" y="245" w="1082" h="42" fontsize="26" selected_fontsize="26" align="hcenter" active="@@act-old_account"/>
<include filename="inc_loading.xml"/>
</page>
